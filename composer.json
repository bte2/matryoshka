{
    "name": "laracasts/matryoshka",
    "description": "Russian Doll Caching for Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "cache"],
    "authors": [
        {
            "name": "Ashwin Rajgopal",
            "email": "ashraj98@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Laracasts\\Matryoshka\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "illuminate/database": "^5.1",
        "illuminate/cache": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laracasts\\Matryoshka\\MatryoshkaServiceProvider"
            ]
        }
    }
}
